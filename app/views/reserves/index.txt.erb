id	title	isbn	item_identifier	call_number	username	created_at	state
<%- @reserves.each do |reserve| -%>
<%= reserve.id %>	"<%= reserve.manifestation.original_title.gsub(/"/, '""') %>"	<%= reserve.manifestation.identifier_contents(:isbn).first %>	<%= reserve.item.try(:item_identifier) %>	<%= reserve.item.try(:call_number) %>	<%= reserve.user.try(:username) %>	<%= reserve.created_at %>	<%= i18n_state(reserve.current_state) %><%= "\n" -%>
<%- end -%>
